    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@types/node": "^14.6.0",
    "@types/shelljs": "^0.8.8",
    "prettier": "^2.0.5",
    "rollup": "^2.26.9",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript2": "^0.27.2",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "homepage": "https://github.com/valjic1/typeorm-simple-history",
  "repository": {
    "type": "git",
    "url": "https://github.com/valjic1/typeorm-simple-history.git"
